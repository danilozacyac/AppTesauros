<Window x:Class="AdministracionTesauro.Inicial"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Administración Tesauro" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Name="Permisos">_Permisos</Button>
        <Button Name="Usuarios" Grid.Column="1" Click="Usuarios_Click">_Usuarios</Button>
        <Grid Grid.Row="1" Grid.ColumnSpan="2" Name="Contenido"/>
    </Grid>
</Window>
